/* custom-tab-bar/tabbar.wxss */
.tab-bar {
  position: fixed;
  bottom: 0;
  left: 125rpx;
  height: 100rpx;
  width: 500rpx;
  display: flex;
  text-align: center;
  z-index: 1;
}
 
 
.tab-bar-item {
  flex: 1;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}