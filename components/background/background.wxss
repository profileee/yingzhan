.top{
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;

}
.left{
    position: absolute;
    left: 0%;
    top: 0;
    width: 100%;
    height: 100%;
}
.right{
    position: absolute;
    right: -10rpx;
    top: 0;
    width: 100%;
    height: 100%;
}
.bottom{
    bottom: 0;
    left: 0;
    position: absolute;
    width: 100%;
    height: 100%;
}
.tabbar{
    position: fixed;
    bottom: 0rpx;
    height: 100%;
    width: 750rpx;
    left: 10rpx;
}