.dan{
    display: flex;
    flex-direction: column;
    background-color: white;
    border-radius: 25rpx;
    width: 90%;
    margin-left: 5%;
    margin-bottom: 10rpx;
    margin-top: 30rpx;
    box-shadow: 10rpx 5rpx 5rpx rgba(0, 0, 0, 0.2);
}
.one{
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    
}
.head{
height: 130rpx;
width: 130rpx;
border-radius: 50%;
border: 5rpx solid rgb(247,192,107);
margin-left: 20rpx;
margin-top: 80rpx;
}
.container{
    display: flex;
    flex-direction: column;
}
.container1{
    display: flex;
    flex-direction: row;
    margin-top: 10rpx;
}
.container2{
    display: flex;
    flex-direction: row;
}
.name{
    margin-left: 10rpx;
    margin-bottom: 10rpx;
    margin-top: 30rpx;
}
.info{
    width: 400rpx;
    height: 180rpx;
    font-size: smaller;
    background-color: rgb(252, 223, 179);
    padding: 10rpx;
    box-sizing: border-box;
    border-radius: 20rpx;

}
.price{
    color: rgb(245, 170, 58);
    font-weight: 500;
    font-size: larger;
    margin-bottom: 30rpx;
    position: relative;
    left: 50rpx;

}